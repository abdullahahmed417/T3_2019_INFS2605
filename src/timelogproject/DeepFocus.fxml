<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="458.0" prefWidth="735.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="timelogproject.DeepFocusController">
    <children>
      <ToolBar prefHeight="58.0" prefWidth="741.0">
         <items>
            <Button fx:id="tlBtn" mnemonicParsing="false" onAction="#handleTLBtnAction" text="Time Log" />
            <Button fx:id="kbBtn" mnemonicParsing="false" onAction="#handleKBBtnAction" text="Kanban Board" />
            <Button fx:id="mlBtn" mnemonicParsing="false" onAction="#handleMLBtnAction" text="My Life" />
            <Button fx:id="wbdBtn" mnemonicParsing="false" onAction="#handleWBdBtnAction" text="Weekly Breakdown" />
            <Button fx:id="dbdBtn" mnemonicParsing="false" onAction="#handleDBdBtnAction" text="Daily Breakdown" />
            <Button mnemonicParsing="false" onAction="#handleHelpButtonAction" text="About + Help?" />
         </items>
      </ToolBar>
      <ComboBox fx:id="comboBoxTasks" layoutX="86.0" layoutY="137.0" onAction="#comboChanged" prefWidth="150.0" promptText="Pick your task" />
      <Button layoutX="592.0" layoutY="390.0" mnemonicParsing="false" text="About + Help?" />
      <Label fx:id="dateTime" layoutX="292.0" layoutY="108.0" prefHeight="112.0" prefWidth="412.0" text="Label">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Label fx:id="taskName" layoutX="455.0" layoutY="253.0" />
      <Label fx:id="taskDescription" layoutX="455.0" layoutY="299.0" />
    </children>
</AnchorPane>
